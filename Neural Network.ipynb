    "#execute model\n",
    "train_df,test_df = s_data.select('UW10', 'UW11', 'UW12', 'UW13', 'UW14', 'UW15', 'UW16', 'UW17', 'UW18', 'UW19', 'UW20', 'UW21', 'UW22', 'UW23', 'UW24','features').randomSplit([0.7,0.3],1213)\n",
